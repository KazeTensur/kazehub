local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "KazeHu ",
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "Welcome to my hub:d",
   LoadingSubtitle = "by Kaze",
   Theme = "Default", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Kazehubv1"
   },

   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },

   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Key Ne:D",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Kaze"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simpl

            }
})

---[TABS]---
local Tab1 = Window:CreateTab("Maintab", 4483362458) 
local Tab2 = Window:CreateTab("Teleport", 4483362458) 
local Tab3 = Window:CreateTab("ThemeIdentifier", 4483362458)

---[SECTIONS]---
local Section = Tab1:CreateSection("Main stuff")
local Section = Tab2:CreateSection("Teleport stuff") 
local Section = Tab3:CreateSection("Theme stuff")

---[BUTONS]---
local Button2 = Tab2:CreateButton({
   Name = "Ishikage",
   Callback = function()
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    humanoidRootPart.CFrame = CFrame.new(1357.25635, 132.093231, -121.109612, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                end
            end
   end,
})

local Button3 = Tab2:CreateButton({
        Name = "Aruka",
        Callback = function()
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    humanoidRootPart.CFrame = CFrame.new(1134.74194, 10.2914276, -1204.85547, -0.00116586685, -0.0604533665, 0.998170316, 0.0194441117, 0.997980952, 0.0604646057, -0.999810278, 0.0194790289, 1.19805336e-05)
                end
            end
        end,
 })

local Button4 = Tab2:CreateButton({
        Name = "Hayake (quest chunin)",
        Callback = function()
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    humanoidRootPart.CFrame = CFrame.new(-2237.86011, -653.359009, 4561.11621, -1, 0, 0, 0, 1, 0, 0, 0, -1)
                end
            end
        end,
})

---[SLIDERS]---
local Slider = Tab1:CreateSlider({
    Name = "Walkspeed",
    Range = {12, 200},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 12,
    Flag = "",
    Visible = false, -- Hide slider initially
    Callback = function(value)
        local humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = value
        end
    end,
})

---[DROPDOWNS]---
local ThemesList = {"AmberGlow", "Amethyst", "Bloom", "DarkBlue", "Green", "Light", "Ocean", "Serenity"}

local ThemeSelector = Tab3:CreateDropdown({
    Name = "UI Theme",
    Options = ThemesList,
    CurrentOption = {"Default"},
    MultipleOptions = false,
    Flag = "",
    Callback = function(Options)
        Window.ModifyTheme(Options[1])
            
    end,
})

Rayfield:LoadConfiguration()
